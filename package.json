{
  "name": "cypress-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "yarn clean:jest && jest --coverage --coverageDirectory=coverage/unit",
    "cy:open": "yarn clean:cy CYPRESS=true start-server-and-test 9000 'cypress open --project e2e'",
    "cy:run": "yarn clean:cy && CYPRESS=true start-server-and-test 9000 'cypress run --project e2e'",
    "clean": "yarn clean:jest && yarn clean:cy",
    "clean:cy": "rm -rf e2e/.nyc_output && rm -rf e2e/coverage",
    "clean:jest": "rm -rf coverage",
    "coverage": "yarn clean && yarn test && yarn cy:run && yarn coverage:merge && yarn coverage:report",
    "coverage:merge": "istanbul-merge --out coverage/merged/coverage-final.json coverage/unit/coverage-final.json e2e/coverage/coverage-final.json",
    "coverage:report": "nyc report --reporter=lcov --reporter=text --temp-dir=coverage/merged --report-dir=coverage/merged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@cypress/code-coverage": "^3.8.1",
    "@testing-library/react": "^10.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-react-simple": "^1.0.2",
    "coveralls": "^3.1.0",
    "css-loader": "^3.6.0",
    "cypress": "^4.8.0",
    "eslint": "7.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-helloitsjoe": "^1.2.4",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.3.0",
    "istanbul-merge": "^1.1.1",
    "jest": "^26.0.1",
    "jest-simple-config": "^0.2.4",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-simple": "^1.6.1"
  }
}
